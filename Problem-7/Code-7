#include<iostream>
using namespace std;
int biggest_finder(int a,int b,int c,int d){
    if (a>b && a>c && a>d){
        return a;
    }
    else if (b>a && b>c && b>d){
        return b;
    }
    else if (c>b && c>b && c>d){
        return c;
    }
    else{
        return d;
    }
}
int main(){
int a,b,c,d;
cin>>a>>b>>c>>d;
cout<<biggest_finder(a,b,c,d);
return 0;
}
