#include <iostream>
using namespace std;

int main() {
    int n, q;
    cin >> n >> q;

    int* arr[n];
    int size[n];

    for (int i = 0; i < n; i++) {
        cin >> size[i];
        arr[i] = new int[size[i]];
        for (int j = 0; j < size[i]; j++) {
            cin >> arr[i][j];
        }
    }

    for (int k = 0; k < q; k++) {
        int a, b;
        cin >> a >> b;
        cout << arr[a][b] << endl;
    }

    for (int i = 0; i < n; i++) {
        delete[] arr[i];
    }

    return 0;
}
